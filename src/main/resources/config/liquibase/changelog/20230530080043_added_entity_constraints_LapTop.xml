<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity LapTop.
    -->
    <changeSet id="20230530080043-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="seller_id"
                                 baseTableName="lap_top"
                                 constraintName="fk_lap_top__seller_id"
                                 referencedColumnNames="id"
                                 referencedTableName="seller"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="lap_top"
                                 constraintName="fk_lap_top__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>
    </changeSet>
</databaseChangeLog>
